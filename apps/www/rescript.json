{
	"name": "www",
	"sources": [
		{
			"dir": "src",
			"subdirs": true
		}
	],
	"package-specs": [
		{
			"module": "es6",
			"in-source": true
		}
	],
	"suffix": ".res.js",
	"bs-dependencies": ["@rescript/core", "@rescript/react"],
	"bsc-flags": ["-open RescriptCore"],
	"jsx": { "version": 4, "mode": "classic" },
	"gentypeconfig": {
		"language": "typescript",
		"shims": {},
		"generatedFileExtension": ".gen.tsx",
		"module": "es6",
		"debug": {
			"all": false,
			"basic": false
		}
	}
}
